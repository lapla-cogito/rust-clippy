#![warn(clippy::cast_lossless)]
#![allow(clippy::char_lit_as_u8)]

type I32 = i32;
type U32 = u32;

fn main() {
    let _ = u32::from('a');
    //~^ cast_lossless
    let _ = 'a' as i32;
    let _ = u64::from('a');
    //~^ cast_lossless
    let _ = 'a' as i64;
    let _ = U32::from('a');
    //~^ cast_lossless
    let _ = 'a' as I32;

    let _ = 'a' as u8;
    let _ = 'a' as i8;
}
